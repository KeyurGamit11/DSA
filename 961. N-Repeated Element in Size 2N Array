Input: nums = [1,2,3,3]
Output: 3
#code:
class Solution {
public:
    int repeatedNTimes(vector<int>& nums) {
        int n = nums.size();
        unordered_map<int,int>mp;
        for(auto &i:nums){
            mp[i]++;
        }
        int ans=0;
        for(auto &[num,freq]:mp){
            if(freq==n/2){
                ans = num;
            }
        }
        return ans;
    }
};
