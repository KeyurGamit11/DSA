Input: root = [1,2,3,4,5,6]
Output: 6
#code:

class Solution {
private:
    int solve(TreeNode* root){
        if(root==NULL){
            return 0;
        }
        int leftcnt = solve(root->left);
        int rightcnt = solve(root->right);
        return leftcnt+rightcnt+1;
    }
public:
    int countNodes(TreeNode* root) {
        return solve(root);
    }
};
