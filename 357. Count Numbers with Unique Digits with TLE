Input: n = 2
Output: 91
Explanation: The answer should be the total numbers in the range of 0 â‰¤ x < 100, excluding 11,22,33,44,55,66,77,88,99
#work for n=0 to n=6
#code:
class Solution {
#define ll long long
private:
    bool unique_digit(ll n){
       vector<int>dig;
        while(n){
            int digit=n%10;
            dig.push_back(digit);
            n /= 10;
        }
        sort(dig.begin() , dig.end());
        bool ch=false;
        for(int i=1; i<dig.size(); i++){
            if(dig[i-1] == dig[i]){
                ch=true;
            }
        }
       if(ch){
        return true;
       }
       return false;
    }
public:
    int countNumbersWithUniqueDigits(int n) {
        ll total=pow(10,n);
        
        cout<<total<<endl;
        ll cnt=0;
        for(int i=11; i<total; i++){
            if(unique_digit(i)){
                cnt++;
            }
        }
        cout<<cnt<<endl;
        return total-cnt;
    }
};
